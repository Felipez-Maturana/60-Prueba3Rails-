<h1>Usuarios#index</h1>
<p>Find me in app/views/usuarios/index.html.erb</p>

<div class="container">
  <h2>Lista de usuarios</h2>             
  <table class="table">
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Email</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
    <% @users.each do |x| %>
      <tr>
        <td><%= x.name %></td>
        <td><%= x.email %></td>
        <td><%= link_to "Canciones" , songsof_path(x) %> </td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>